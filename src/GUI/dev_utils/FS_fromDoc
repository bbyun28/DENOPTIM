#!/bin/bash
./addParamCode.sh SrcOrNew B NONE "Use existing fragment space:" 0 "Tick here to use a previously defined fragment space." FSParametersForm.java
./addParamCode.sh FSSource F NONE "Fragment space file:" 1 "Pathname of the file defining the fragment space. This file is generated by the DENOPTIM's GUI upon construction of a new fragment space." FSParametersForm.java
./addParamCode.sh Par1 F FS-ScaffoldLibFile "Scaffolds/root fragments library:" 2 "Pathname of the file containing the list of scaffolds." FSParametersForm.java
./addParamCode.sh Par2 F FS-FragmentLibFile "Fragments library:" 2 "Pathname of the file containing the list of fragments." FSParametersForm.java
./addParamCode.sh Par3 F FS-CappingFragmentLibFile "Capping groups library:" 2 "Pathname of the file containing the list of capping groups." FSParametersForm.java
./addParamCode.sh Par4 F FS-CompMatrixFile "Compatibility matrix file:" 2 "Pathname of the compatibility matrix file. Note that this file contains the compatibility matrix and related information such as the AP-Class to bond order map, the capping map, and the list of forbidden ends." FSParametersForm.java
./addParamCode.sh Par6 F FS-RotBondsDefFile "Rotatable bonds list:" 2 "Pathname of the file containing the definition of the rotatable bonds. Must be a list of SMARTS." FSParametersForm.java
./addParamCode.sh Par7 S FS-MaxHeavyAtom "Max non-H atoms:" 2 "Maximum number of heavy (non-hydrogen) atoms for a candidate." FSParametersForm.java
./addParamCode.sh Par8 S FS-MaxMW "Max molecular weight: " 2 "Maximum molecular weight accepted for a candidate" FSParametersForm.java
./addParamCode.sh Par9 S FS-MaxRotatableBond "Max rotatable bonds:" 2 "Maximum number of rotatable bonds accepted for a candidate" FSParametersForm.java
./addParamCode.sh Par10 S FS-EnforceSymmetry "Enforce symmetry:" 2 "Forces constitutional symmetry whenever possible. Corresponds to setting the symmetric substitution probability to 100%." FSParametersForm.java
./addParamCode.sh Par11 S FS-ConstrainSymmetry "Symmetry constraints:" 2 "<html>TODO(toTable): List of constraints in the symmetric substitution probability. The constraints defined by this keyword overwrite the symmetric substitution probability defined by DenoptimGA, and the requirement of the <code>FS-EnforceSymmetry</code> keyword.</html>" FSParametersForm.java
